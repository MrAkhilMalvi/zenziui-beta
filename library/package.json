{
  "name": "zenziui",
  "version": "1.0.2",
  "description": "A reusable React + TypeScript UI component library.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup ../components/zenziui/index.ts --format esm,cjs --dts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "@radix-ui/react-dialog": "^1.1.15"
  },
  "author": "Akhil Malviya",
  "license": "MIT",
  "keywords": ["react", "ui", "components", "zenziui"]
}
